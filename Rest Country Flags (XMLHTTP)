//Use the rest countries API url -> https://restcountries.com/v2/all and display all the country flags in console

let request= new XMLHttpRequest();

//GET command in XMLHTTP fetchs the data from server

request.open ('GET', "https://restcountries.com/v2/all" );
request.send ();

request.onload = () => {

let responseContent=JSON.parse(request.response)
responseContent.map((x) => {
  console.log(x.flag)
})
}

